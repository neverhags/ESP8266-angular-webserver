{
  "name": "www-src",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng lint && ng build --prod --aot true --bundleStyles false --keepStyles false --output-hashing none --single-bundle true --extra-webpack-config webpack.partial.js -o && npm run post-build:bundle && npm run post-build:uglify && npm run post-build:clean && npm run post-build:overwrite",
    "post-build:bundle": "echo 'Document=document;' > ../data/bundle.js && echo \"\n\" >> ../data/bundle.js && cat ../data/polyfills-es5.js >> ../data/bundle.js && echo \"\n\" >> ../data/bundle.js && cat ../data/polyfills.js >> ../data/bundle.js && echo \"\n\" >> ../data/bundle.js && cat ../data/main.js >> ../data/bundle.js",
    "post-build:uglify": "npx uglifyjs --compress --mangle -mt -- ../data/bundle.js  > ../data/bundle.min.js",
    "post-build:clean": "rm ../data/polyfills-es5.js && rm ../data/polyfills.js && rm ../data/main.js && rm ../data/bundle.js && rm ../data/3rdpartylicenses.txt",
    "post-build:overwrite": "cp post-build/index.html ../data/index.html",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.0.2",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "ngx-build-plus": "^9.0.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "uglify-js": "^3.9.4"
  }
}
